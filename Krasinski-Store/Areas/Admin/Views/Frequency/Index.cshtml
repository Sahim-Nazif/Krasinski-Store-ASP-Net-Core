
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="border">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Frequency List</h2>
        </div>
        <div class="col-6 text-center">
            <a asp-action="Upsert" class="btn btn-dark"><i class="fas fa-plus"> Add Frequency </i></a>
        </div>
    </div>
    <br />
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Frequency Count</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section scripts {
    <script src="~/js/frequency.js"></script>
}

